{
    "name": "@rauzeebr/media-library-pro-styles",
    "version": "1.3.0",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/Rauzee/laravel-medialibrary-pro.git",
        "directory": "resources/js/media-library-pro-styles"
    },
    "main": "dist/styles.css",
    "scripts": {
        "build": "postcss src/styles.css --output dist/styles.css --env production --use tailwindcss --verbose",
        "build:dev": "postcss src/styles.css --output dist/styles.css --use tailwindcss --verbose",
        "watch": "postcss src/styles.css --output dist/styles.css --watch --use tailwindcss --verbose",
        "run-link": "yarn link"
    },
    "devDependencies": {
        "postcss": "^7.0.35",
        "postcss-cli": "^7.1.1",
        "tailwindcss": "^1.9.6"
    }
}
